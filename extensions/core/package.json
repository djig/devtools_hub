{
  "name": "@devtools-hub/core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && npm run build:types",
    "build:types": "cp src/index.d.ts dist/index.d.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "js-yaml": "^4.1.1",
    "fast-xml-parser": "^5.3.2",
    "papaparse": "^5.5.3",
    "markdown-it": "^14.1.0",
    "turndown": "^7.2.2",
    "js-beautify": "^1.15.4",
    "crypto-js": "^4.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/js-beautify": "^1.14.3",
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/papaparse": "^5.5.0",
    "@types/turndown": "^5.0.6",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.0.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16"
  }
}
